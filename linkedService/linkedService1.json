		"parameters": {
			"DB Server": {
				"type": "String",
				"defaultValue": "192.168.56.1"
			},
			"db port": {
				"type": "String",
				"defaultValue": "2311"
			},
			"DB Name": {
				"type": "String",
				"defaultValue": "family"
			},
			"Db User": {
